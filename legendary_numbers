number  =int(input("Enter a number:"))
for i in range(1,number+1):
    if i % 3 == 0 and i % 5 == 0:
        print(f" {i}  This is a legendary number")
    elif i % 3 == 0:
        print(f" {i}  This is a magical number")
    elif i % 5 == 0:
        print(f" {i}  This is a cursed number")
    else:
        print(f" {i}  This is a normal number")
